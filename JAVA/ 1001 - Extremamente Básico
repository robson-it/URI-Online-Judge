import java.io.IOException;
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) throws IOException {
        
        Scanner entrada = new Scanner(System.in);
		int A, B, X;
		
		A=entrada.nextInt();
		B=entrada.nextInt();
		X=A+B;
		
		System.out.println("X = "+X);
 
    }
 
}
